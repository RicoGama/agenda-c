{
    "version": "2.0.0",
    "tasks": [
      {
        "label": "C/C++: gcc-9 arquivo de build ativo",
        "type": "shell",
        "command": "/usr/bin/gcc-9",
        "args": [
          "-fdiagnostics-color=always",
          "-g",
          "${file}",
          "-o",
          "${fileDirname}/${fileBasenameNoExtension}"
        ],
        "options": {
          "cwd": "${fileDirname}"
        },
        "group": {
          "kind": "build",
          "isDefault": true
        }
      }
    ]
  }
  